{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/include",
            "/usr/include/x86_64-linux-musl"
        ],
        "myDefines": [
            "DEBUG",
            "MY_FEATURE=1",
            "_POSIX_C_SOURCE=199506L",
            "_XOPEN_SOURCE=500",
            "PLATFORM_LINUX",
            "MUSL",
            "NDEBUG"
        ]
    },
    "configurations": [
        {
            "name": "Linux AMD64 (musl-gcc)",
            "compilerPath": "/usr/bin/musl-gcc",
            "compilerArgs": [
                "-std=c90",
                "-D_POSIX_C_SOURCE=199506L",
                "-D_XOPEN_SOURCE=500",
                "-ansi",
                "-O3",
                "-Wall",
                "-Wextra",
                "-pedantic",
                "-I/usr/include/x86_64-linux-musl",
                "-I${workspaceFolder}/include",
                "-L/usr/lib/x86_64-linux-musl",
                "-static",
                "-fno-common",
                "-ffunction-sections",
                "-fdata-sections",
                "fstack-protector-strong",
                "-fPIC",
                "-static"
            ],
            "intelliSenseMode": "linux-gcc-x64",
            "includePath": [
                "${myIncludePath}"
            ],
            "defines": [
                "${myDefines}"
            ],
            "cStandard": "c89",
            "browse": {
                "path": [
                    "${myIncludePath}",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}
